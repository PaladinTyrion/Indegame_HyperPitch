    init=1;
    catx=obj_sabot.x-16;
    z=-64-9;
    catz=-64;
    if (nopain)
        catx-=128;
    if (!global.nosound){
        sound_stop(snd_catstrike);
        sound_play(snd_catstrike);
        }
    obj_pitch.countering=0;
    obj_pitch.counter=0;
    obj_pitch.misscounter=0;
    obj_pitch.docounter=0;
    obj_pitch.firing=0;
    obj_pitch.firingweapon=0;
    }

//CATSTRIKE
catx+=10;
catanim+=1;
if (catanim==4)
    catimg=1;
if (catanim==8){
    catanim=0;
    catimg=0;
    }
draw_sprite(spr_catstrike,2+catimg,catx,y+catz);

//PITCH
if (catx&lt;x)
    draw_sprite(global.pitchsprite,0,catx+1,y+catz-9);
else if (!jumped){
    jumped=1;
    zgrav=0.3;
    zspeed=-3;
    }
if (jumped){
    zspeed+=zgrav;
    z+=zspeed;
    draw_sprite(global.pitchsprite,72,x,y+z);
    //Ending special case
    if (z+zspeed+zgrav&gt;=0 &amp;&amp; ending){
        instance_destroy();
        obj_ending.pitchlanded=1;
        }
    //Normal spawn
    else if (z+zspeed+zgrav&gt;=0 &amp;&amp; !ending){
        if (global.level==1 &amp;&amp; obj_sabot.roomx=0 &amp;&amp; obj_sabot.roomy=4)
            scr_wantsong(1);
        instance_destroy();
        global.ignoreall=0;
        obj_pitch.visible=1;
        if (obj_sabot.snowfall){
            if (!global.nosound)
                sound_play(snd_snowstep2);
            repeat(8)
                tempid=(instance_create(x-2+random(6),y-1+random(3)+6,obj_snowpart));
            }
        if (!nopain){
            obj_pitch.pain=120;
            obj_pitch.altpain=120;
            with (obj_enemy){
                if (!boss &amp;&amp; point_distance(x,y,obj_pitch.x,obj_pitch.y)&lt;48){
                    obj_pitch.alarm[4]=1;
                    (instance_create(x,y,obj_nukeexplo)).nopoints=1;
                    hp=0;
                    killtype=4;
                    killdir=random(360);
                    nopoints=1;
                    }
                }
            with (obj_enemybullet){
                obj_pitch.alarm[3]=1;
                instance_destroy();
                instance_create(x,y,obj_elfexplo);
                }
            }
        }
    }
