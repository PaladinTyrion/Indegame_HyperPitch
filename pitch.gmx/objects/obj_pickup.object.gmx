zspeed=3;
z=5;
depth=326-y+obj_sabot.y;

scr_pickupweights();
//Random
r=floor(random(w1+w2+w3+w4+w5+w6+w7));
//Random powerup
r2=floor(random(5))+10;
//if (random(1)&lt;0.1)
//    r2=14;
//Random score bonus
r3=floor(random(3));
if (random(1)&lt;0.1)
    r3=3;

//CHOOSE THE PICKUP
     if (r&lt;w1) image_single=r3;
else if (r&gt;=w1 &amp;&amp; r&lt;w1+w2) image_single=4;
else if (r&gt;=w1+w2 &amp;&amp; r&lt;w1+w2+w3) image_single=5;
else if (r&gt;=w1+w2+w3 &amp;&amp; r&lt;w1+w2+w3+w4) image_single=6;
else if (r&gt;=w1+w2+w3+w4 &amp;&amp; r&lt;w1+w2+w3+w4+w5) image_single=7;
else if (r&gt;=w1+w2+w3+w4+w5 &amp;&amp; r&lt;w1+w2+w3+w4+w5+w6) image_single=8;
else image_single=r2;

//ONLY ONE CATSTRIKE
if (image_single==7){
    global.onecat=0;
    with (obj_pickup){
        if (image_single==7)
            global.onecat+=1;
        }
    if (global.onecat&gt;1)
        instance_destroy();
    }

//HYPER XYZ PICKUP - OVERRIDES ALL OTHERS
if (random(1)&lt;0.0025)
    image_single=16+floor(random(3));

x+=round(-1+random(2));
y+=round(-1+random(2));
wantx=x;
wanty=y;
//Jump to position
if (!place_free(x,y) || place_meeting(x,y,obj_pitchblock)){
    savex=x;
    savey=y;
    with (obj_pitchblock)
        solid=1;
    move_outside_solid(point_direction(x,y,obj_pitch.x,obj_pitch.y),400);
    with (obj_pitchblock)
        solid=0;
    wantx=x;
    wanty=y;
    x=savex;
    y=savey;
    //The jump calculation
    direction=point_direction(x,y,wantx,wanty);
    speed=point_distance(x,y,wantx,wanty)/17;
    }

if (x&lt;obj_sabot.x+8 || x&gt;obj_sabot.x+312 || y&lt;obj_sabot.y+8 || y&gt;obj_sabot.y+232)
    instance_destroy();
if (wantx&lt;obj_sabot.x+8 || wantx&gt;obj_sabot.x+312 || wanty&lt;obj_sabot.y+8 || wanty&gt;obj_sabot.y+232)
    instance_destroy();
</string>
