            <string>//MOVE AND ANIMATE
depth=320-y+obj_sabot.y;
movecyc+=1;
//Waiting, jump
if (state==0 &amp;&amp; movecyc==2)
    image_single=1;
else if (state==0 &amp;&amp; movecyc==4)
    image_single=0;
else if (state==0 &amp;&amp; movecyc==waittime-6)
    image_single=1;
else if (state==0 &amp;&amp; movecyc==waittime-4)
    image_single=2;
else if (state==0 &amp;&amp; movecyc==waittime-2)
    image_single=1;
else if (state==0 &amp;&amp; movecyc&gt;=waittime){
    if (!global.nosound)
        sound_play(snd_movewhoosh);
    image_single=3;
    invincible=1;
    movecyc=0;
    state=1;
    zgrav=-0.2;
    zspeed=4;
    jumpstate=1-jumpstate;
    if (jumpstate==0){
        wantx=obj_pitch.x;
        wanty=obj_pitch.y;
        }
    else{
        wantx=obj_pitch.x-35+random(70);
        wanty=obj_pitch.y-35+random(70);
        while (!place_free(wantx,wanty) || wantx&lt;obj_sabot.x+32 || wanty&lt;obj_sabot.y+48){
            wantx=obj_pitch.x-35+random(70);
            wanty=obj_pitch.y-35+random(70);
            }
        }
    hspeed=(wantx-x)/20;
    vspeed=(wanty-y)/20;
    }
//Jumping and falling
else if (state==1){
    if (movecyc==20){
        speed=0;
        zgrav=-1.5;
        zspeed=0;
        }
    if (zspeed&lt;0 &amp;&amp; z+zspeed+zgrav&lt;=8)
        invincible=0;
    //Land
    if (zspeed&lt;0 &amp;&amp; z+zspeed+zgrav&lt;=0){
        image_single=2;
        movecyc=0;
        state=0;
        invincible=0;
        z=0;
        zspeed=0;
        zgrav=0;
        scr_explo2();
        global.shakescreen+=10;
        for (i=0;i&lt;360;i+=20){
            tempid=instance_create(x,y+6,obj_smoke);
            tempid.direction=i;
            tempid.speed=6;
            tempid.vspeed*=0.8;
            }
        if (instance_number(obj_crackedfloor)&lt;150)
            instance_create(x,y+6,obj_crackedfloor);
        }
    }
zspeed+=zgrav;
z+=zspeed;

//HITFLASH
if (hitflash&gt;0)
    hitflash-=1;

//DIE
if (hp&lt;=0){
    global.stats[0]+=1;
    if (!nopoints){
        global.points+=points*global.multiplier;
        if (global.showscore)
            (instance_create(x,y-4,obj_scorenumber)).points=points*global.multiplier;
        instance_create(x,y,obj_pickup);
        }
    if (killtype==1)
        killtype=4;
    //ICE
    if (killtype==2){
        instance_destroy();
        if (global.shakescreen&lt;4)
            global.shakescreen+=2;
        instance_create(x,y,obj_iceflash);
        repeat(8){
            tempid=instance_create(x,y,obj_icegib);
            tempid.direction=killdir-45+random(90);
            }
        for (i=0;i&lt;4;i+=1){
            tempid=instance_create(x,y,obj_icepuff);
                tempid.direction=i*90+45;
                tempid.speed=7;
            }
        if (megaice || random(1)&lt;0.1){   //Power ice
            scr_icekillbig();
            scr_megaicefx();
            tempid=instance_create(x,y,obj_elfgib);
                tempid.type=floor(random(3))+8;
                tempid.direction=random(360);
                tempid.speed=0.3+random(0.8);
                tempid.vspeed*=0.85;
                tempid.zspeed=random(5)+3;
            tempid=instance_create(x,y,obj_elfgib);
                tempid.type=floor(random(4))+11;
                tempid.direction=random(360);
                tempid.speed=0.3+random(0.8);
                tempid.vspeed*=0.85;
                tempid.zspeed=random(5)+3;
            tempid=instance_create(x,y,obj_elfgib);
                tempid.type=15;
                tempid.direction=random(360);
                tempid.speed=0.3+random(0.8);
                tempid.vspeed*=0.85;
                tempid.zspeed=random(5)+3;
            }
        else
            scr_icekill();
        }
    //LASER
    else if (killtype==3){
        scr_rainbowkill();
        instance_destroy();
        global.flash=1;
        if (global.bake[4]==0)
            scr_lxkill(spr_lx_microelfv,spr_lx_microelfh,19);
        else{
            for (i=0;i&lt;7;i+=1){
                tempid=instance_create(x,y,obj_laserexplo);
                tempid.img4=i;
                tempid.speed=(-6+i*2)*3;
                tempid.direction=killdir+90;
                tempid.image_single=tempid.img4*9;
                if (hyperlaser){
                    tempid.sprite_index=spr_hyperlaserexplo;
                    tempid.image_single=0;
                    }
                }
            }
        }
    //NUKE
    if (killtype==1 || killtype==4){
        instance_destroy();
        instance_create(x,y,obj_bigexplo);
        scr_explo2();
        if (global.shakescreen&lt;4)
            global.shakescreen+=2;
        tempid=instance_create(x,y,obj_elfgib);
            tempid.type=floor(random(3));
            tempid.direction=random(360);
            tempid.speed=0.1+random(1.8);
            tempid.vspeed*=0.85;
            tempid.zspeed=random(6)+3;
        tempid=instance_create(x,y,obj_elfgib);
            tempid.type=floor(random(4))+3;
            tempid.direction=random(360);
            tempid.speed=0.1+random(1.8);
            tempid.vspeed*=0.85;
            tempid.zspeed=random(6)+3;
        tempid=instance_create(x,y,obj_elfgib);
            tempid.type=7;
            tempid.direction=random(360);
            tempid.speed=0.1+random(1.8);
            tempid.vspeed*=0.85;
            tempid.zspeed=random(6)+3;
        }
    }

if (x&lt;obj_sabot.x-8 || x&gt;obj_sabot.x+328 || y&lt;obj_sabot.y-8 || y&gt;obj_sabot.y+248)
    instance_destroy();
