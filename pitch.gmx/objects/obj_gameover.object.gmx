            <string>//Init ripple
if (!init){
    init=1;
    for (i=0;i&lt;55;i+=1)
        ripple[i]=0;
    if (obj_santa.delaygameover==1)
        cyc=-110;
    else
        cyc=0;
    }

//Always calc ripple
rcyc+=1;
     if (rcyc== 1) ripple[0]=-1;
else if (rcyc== 2) ripple[0]=-3;
else if (rcyc== 3) ripple[0]=-4;
else if (rcyc== 4) ripple[0]=-5;
else if (rcyc== 5) ripple[0]=-5;
else if (rcyc== 6) ripple[0]=-5;
else if (rcyc== 7) ripple[0]=-4;
else if (rcyc== 8) ripple[0]=-3;
else if (rcyc== 9) ripple[0]=-1;
else if (rcyc==10) ripple[0]=1;
else if (rcyc==11) ripple[0]=3;
else if (rcyc==12) ripple[0]=4;
else if (rcyc==13) ripple[0]=5;
else if (rcyc==14) ripple[0]=5;
else if (rcyc==15) ripple[0]=5;
else if (rcyc==16) ripple[0]=4;
else if (rcyc==17) ripple[0]=3;
else if (rcyc==18){ ripple[0]=1; rcyc=0;}
for (i=55;i&gt;0;i-=1)
    ripple[i]=ripple[i-1];

//STATE: Wait
if (state==0){
    csimg=5;
    cyc+=1;
    if (cyc==15){
        cyc=0;
        state=1;
        }
    }

//STATE: Slam down
else if (state==1){
    if (csimg&lt;5){
        if (!csimgcyc)
            csimgcyc=1;
        else{
            csimgcyc=0;
            csimg+=1;
            }
        }
    //Stop, sparks
    if (cyc&lt;8){
        cyc+=1;
        if (cyc==8 &amp;&amp; substate==0){
            scr_wantsong(11);
            shakescreen=10;
            //Make sound according to option, since regular sound is off
            if (!global.nomenusound){
                sound_stop(snd_explo3);
                sound_play(snd_explo3);
                }
            csimg=0;
            substate=1;
            repeat(50)
                instance_create(x+126+random(72),y+119,obj_gameoverpart);
            repeat(50){
                (instance_create(x+126,y+119,obj_gameoverpart)).fire=1;
                (instance_create(x+198,y+119,obj_gameoverpart)).fire=2;
                }
            }
        }
    //Next state
    if (substate){
        subcyc+=1;
        if (subcyc==50)
            state=2;
        }
    //Draw
    for (i=0;i&lt;10;i+=1){
        for (j=-1;j&lt;5;j+=1)
            draw_sprite(spr_coverscreen,5,x+i*32,y+j*24-256+cyc*32);
        if (substate==1)
            draw_sprite(spr_coverscreen,csimg,x+i*32,y+119);
        for (j=-1;j&lt;5;j+=1)
            draw_sprite(spr_coverscreen,5,x+i*32,y-j*24+472-cyc*32);
        }
    for (i=0;i&lt;27;i+=1){
        draw_sprite(spr_gameover,i,x+160,y+i-27-136+cyc*32);
        draw_sprite(spr_gameover,i+27,x+160,y+i+376-cyc*32);
        }
    }

//STATE: Disappear
else if (state==2){
    endcyc+=1;
    if (endcyc==118-40)
        scr_wantsong(0);
    if (endcyc==120){
        global.latestpoints=global.points;
        scr_restartgame(0,0);
        }
    if (cyc2&lt;1000)
        cyc2+=0.01+cyc2/10;
    for (i=0;i&lt;10;i+=1){
        for (j=-1;j&lt;11;j+=1)
            draw_sprite(spr_coverscreen,5,x+i*32,y+j*24);
        }
    for (i=0;i&lt;54;i+=1)
        draw_sprite(spr_gameover,i,x+160+(ripple[i]*cyc2),y+93+i);
    }

//Shake screen
if (shakescreen&gt;0){
    shakescreen-=1;
    if (shakescreen&gt;4)
        factor=4;
    else if (shakescreen&gt;2)
        factor=2;
    else
        factor=1;
    shakeoff=(shakescreen mod 2)*factor;
    if (global.noshake)
        shakeoff=0;
    if (global.gmversion==5)
        view_top[0]=y+shakeoff;
    else if (global.gmversion==8)
        view_yview[0]=y+shakeoff;
    }
