            <string>//HITFLASH
if (hitflash&gt;0)
    hitflash-=1;

if (hp&lt;=0){
    //DESPERATION
    if (global.difficulty&gt;=8 &amp;&amp; !desperation){
        desperation=1;
        obj_sabot.desperation=1;
        hp=maxhp/2;
        global.shakescreen=20;
        obj_ultrasleighgun.hp=-100;
        with (obj_enemybullet){
            instance_destroy();
            instance_create(x,y,obj_elfexplo);
            }
        instance_create(obj_sabot.x,obj_sabot.y,obj_whiteflash);
        if (!global.nosound)
            sound_play(snd_explo4);
        image_single=2;
        for (i=0;i&lt;20;i+=1){
            tempid=instance_create(x,y,obj_bigexplo);
            tempid.speed=6;
            tempid.direction=i*18;
            tempid.vspeed*=0.7;
            tempid.depth-=30;
            }
        repeat(5){
            tempid=instance_create(x-64+random(128),y-25+random(50),obj_hurtexplo);
            tempid.spawndepth=-30;
            tempid.hurt=1;
            }
        repeat(32){
            tempid=instance_create(x-32+random(64),y-12+random(24),obj_elfgib);
            tempid.type=7;
            tempid.direction=random(360);
            tempid.speed=1+random(3);
            tempid.vspeed*=0.85;
            tempid.zspeed=random(6)+3;
            }
        }
    
    //DEATH
    else{
        global.stats[0]+=1;
        global.points+=points*global.multiplier;
        if (global.showscore){
            tempid=instance_create(x,y-16,obj_scorenumber);
            tempid.points=points*global.multiplier;
            tempid.zgrav/=4;
            tempid.slow=4;
            }
        obj_sabot.bosspresent=0;
        obj_ultrasleighgun.hp=-100;
        instance_destroy();
        instance_create(x,y,obj_ultrasleigh2);
        global.shakescreen=20;
        with (obj_enemybullet){
            instance_destroy();
            instance_create(x,y,obj_elfexplo);
            }
        }
    }

//DESPERATION ATTACK
if (desperation &amp;&amp; hp&gt;0){
    //SMOKE AND FIRE
    instance_create(x-48+random(96),y-16+random(32),obj_smoke);
    if (global.difficulty==32 &amp;&amp; despcyc&gt;=80){
        dfcyc+=1;
        if (dfcyc mod 4==0){
            tempid=instance_create(x-48+random(96),y-16+random(32),obj_enemybullet2);
            tempid.hspeed=-(2+random(1));
            tempid.direction=135+random(90);
            }
        if (dfcyc==8){
            dfcyc=0;
            if (!global.nosound)
                sound_play(snd_enemyfire1);
            }
        }
    //MOVE
    depth=290-y+obj_sabot.y;
    if (despcyc&lt;100)
        despcyc+=1;
    if (despcyc==40 &amp;&amp; !global.nosound)
        sound_play(snd_enemychargesmall);
    if (despcyc==80){
        gravity=0.2;
        gravity_direction=180;
        }
    if (hspeed&lt;-5)
        hspeed=-5;
    //HIT WALL
    if (x+hspeed&lt;obj_sabot.x+90){
        hspeed=5;
        if (global.difficulty==32)
            dmvspeed=8;
        else
            dmvspeed=2;
        if (global.shakescreen&lt;8)
        global.shakescreen=6;
        if (!global.nosound)
            sound_play(snd_explo3);
        if (!bouncing){
            bouncing=1;
            vspeed=(floor(random(2))*0.5)-0.25;
            }
        else{
            if (vspeed&lt;0 &amp;&amp; vspeed&gt;-dmvspeed){
                vspeed-=0.5;
                if (vspeed&lt;=-dmvspeed)
                    vspeed=-dmvspeed;
                }
            else if (vspeed&gt;0 &amp;&amp; vspeed&lt;dmvspeed){
                vspeed+=0.5;
                if (vspeed&gt;=dmvspeed)
                    vspeed=dmvspeed;
                }
            }
        }
    if (y+vspeed&lt;obj_sabot.y+48){
        vspeed=abs(vspeed);
        if (global.shakescreen&lt;8)
        global.shakescreen=6;
        if (!global.nosound)
            sound_play(snd_explo3);
        }
    else if (y+vspeed&gt;obj_sabot.y+160){
        vspeed=-abs(vspeed);
        if (global.shakescreen&lt;8)
        global.shakescreen=6;
        if (!global.nosound)
            sound_play(snd_explo3);
        }
    }

//ORDERS SIR
else if (!desperation){
    if (startwait&lt;80){
        startwait+=1;
        if (hspeed&lt;0)
            hspeed+=1;
        if (startwait==60)
            scr_wantsong(8);
        if (startwait==80){
            obj_sabot.bosspresent=1;
            with (obj_elf)
                instance_destroy();
            }
        }
    else{
        movecyc-=1;
        if (movecyc&lt;=0){
            movecyc=100;
            targety=ystart-40+random(80);
            stopmove=0;
            failsafe=0;
            if (!global.nosound)
                sound_play(snd_movewhoosh);
            while (abs(y-targety)&lt;24 || failsafe&gt;=1000){
                targety=ystart-40+random(80);
                failsafe+=1;
                }
            }
        //FIRE
        firecyc+=1;
        //Rapid shot
        if (hp&gt;0){
            if (firecyc mod 2==0 &amp;&amp; (firecyc&lt;=rapid || (firecyc&gt;100 &amp;&amp; firecyc&lt;=100+rapid))){
                if (!global.nosound){
                    sound_stop(snd_enemyfire1);
                    sound_play(snd_enemyfire1);
                    }
                tempid=instance_create(x+13,y-4,obj_enemybullet);
                tempid.direction=180;
                tempid.speed=bulletspeed;
                }
            //Homing shot 1 - Normal and above only
            if (firecyc==20 &amp;&amp; global.difficulty&gt;=3){
                if (!global.nosound){
                    sound_stop(snd_enemyfire1);
                    sound_play(snd_enemyfire1);
                    }
                tempid=instance_create(x+34,y-8,obj_enemybullet3);
                tempid.direction=135;
                }
            //Homing shot 2
            if (firecyc==120){
                if (!global.nosound){
                    sound_stop(snd_enemyfire1);
                    sound_play(snd_enemyfire1);
                    }
                tempid=instance_create(x+29,y+9,obj_enemybullet3);
                tempid.direction=225;
                }
            //Spreadshot
            if (firecyc==40){
                if (!global.nosound){
                    sound_stop(snd_enemyfire2);
                    sound_play(snd_enemyfire2);
                    }
                tempid=instance_create(x,y+6,obj_enemybullet2);
                tempid.direction=180;
                tempid.speed=bulletspeed;
                iter=global.difficulty-1;
                if (iter&lt;1)
                    iter=1;
                if (iter&gt;5)
                    iter=5;
                for (i=1;i&lt;iter;i+=1){
                    tempid=instance_create(x,y+6,obj_enemybullet2);
                    tempid.direction=180-15*i;
                    tempid.speed=bulletspeed;
                    tempid=instance_create(x,y+6,obj_enemybullet2);
                    tempid.direction=180+15*i;
                    tempid.speed=bulletspeed;
                    }
                }
            //Accelerating shot - Hard and above only
            if (firecyc==140 &amp;&amp; global.difficulty&gt;=4){
                if (!global.nosound){
                    sound_stop(snd_enemyfire2);
                    sound_play(snd_enemyfire2);
                    }
                tempid=instance_create(x+14,y+6,obj_enemybullet2);
                tempid.direction=point_direction(x+14,y+6,obj_pitch.x,obj_pitch.y);
                tempid.speed=bulletspeed*0.25;
                tempid.gravity=0.1;
                tempid.gravity_direction=tempid.direction;
                tempid=instance_create(x+14,y+6,obj_enemybullet2);
                tempid.direction=point_direction(x+14,y+6,obj_pitch.x,obj_pitch.y)-5;
                tempid.speed=bulletspeed*0.25;
                tempid.gravity=0.1;
                tempid.gravity_direction=tempid.direction;
                tempid=instance_create(x+14,y+6,obj_enemybullet2);
                tempid.direction=point_direction(x+14,y+6,obj_pitch.x,obj_pitch.y)+5;
                tempid.speed=bulletspeed*0.25;
                tempid.gravity=0.1;
                tempid.gravity_direction=tempid.direction;
                }
            }
        if (firecyc==200)
            firecyc=0;
        }
    
    //MOVE
    depth=290-y+obj_sabot.y;
    if (abs(y-targety)&gt;5)
        y+=ceil((targety-y)/5);
    else
        stopmove=1;
    if (stopmove){
        bob+=1;
        if (bob==4)
            y+=1;
        else if (bob==12)
            y-=1;
        else if (bob==16)
            y-=1;
        else if (bob==24){
            y+=1;
            bob=0;
            }
        }
    else{
        if (y&gt;yprevious)
            bob=20;
        else
            bob=8;
        }
    }</string>
