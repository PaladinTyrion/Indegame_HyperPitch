            //FIRE
            if (!global.nosound)
                sound_play(snd_enemyfire1);
            rot1+=2;
            if (rot1==360)
                rot1=0;
            for (i=0;i&lt;360;i+=45){
                tempid=instance_create(x,y,obj_enemybullet);
                tempid.speed=1.5;
                tempid.direction=i+rot1;
                }
