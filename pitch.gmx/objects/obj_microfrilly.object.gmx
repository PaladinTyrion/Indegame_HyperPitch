        instance_create(x,y,obj_bigexplo);
        }
    //LASER
    else if (killtype==3){
        instance_destroy();
        tempid=instance_create(x,y,obj_explodingfrilly);
        tempid.dir=dir;
        tempid.image_single=1+dir*2;
        instance_create(x,y,obj_bigexplo);
        }
    //NUKE
    if (killtype==1 || killtype==4){
        instance_destroy();
        tempid=instance_create(x,y,obj_explodingfrilly);
        tempid.dir=dir;
        tempid.image_single=1+dir*2;
        instance_create(x,y,obj_bigexplo);
